{
  "name": "@parse/node-apn",
  "description": "An interface to the Apple Push Notification service for Node.js",
  "version": "10.1.0",
  "author": "Parse Platform, Andrew Naylor <argon@mkbot.net>",
  "keywords": [
    "apple",
    "push",
    "push notifications",
    "iOS",
    "apns",
    "notifications"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/parse-community/node-apn.git"
  },
  "dependencies": {
    "debug": "4.3.4",
    "jsonwebtoken": "9.0.2",
    "node-forge": "1.3.1",
    "verror": "1.10.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "11.1.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.2.3",
    "@semantic-release/npm": "11.0.1",
    "@semantic-release/release-notes-generator": "12.1.0",
    "@types/node": "20.9.1",
    "chai": "4.3.10",
    "chai-as-promised": "7.1.1",
    "codecov": "3.8.2",
    "eslint": "8.54.0",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "prettier": "3.1.0",
    "semantic-release": "22.0.8",
    "sinon": "17.0.1",
    "sinon-chai": "3.7.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "coverage": "nyc --reporter=lcov mocha 'test/**/*.js'",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier",
    "eslint": "eslint --cache ./",
    "eslint:fix": "eslint --cache --fix ./",
    "prettier": "prettier --write {lib,test,examples,mock}/{**/*,*}.js index.js",
    "test": "mocha 'test/**/*.js'",
    "update-deps": "rm -rf ./node_modules && rm -rf ./package-lock.json && ncu -u && npm install" 
  },
  "jshintConfig": {
    "node": true
  },
  "eslintConfig": {
    "ecmaVersion": 6,
    "env": {
      "es6": true,
      "node": true
    }
  },
  "engines": {
    "node": ">= 14"
  },
  "license": "MIT"
}
