{
  "name": "node-apn",
  "description": "An interface to the Apple Push Notification service for Node.js",
  "version": "10.8.0",
  "author": "Sam Ilic, Parse Platform, Andrew Naylor <argon@mkbot.net>",
  "keywords": [
    "apple",
    "push",
    "push notifications",
    "iOS",
    "apns",
    "notifications"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/stage88/node-apn.git"
  },
  "dependencies": {
    "debug": "4.4.3",
    "jsonwebtoken": "9.0.3",
    "node-forge": "1.3.3",
    "verror": "1.10.1"
  },
  "devDependencies": {
    "@types/node": "24.10.2",
    "chai": "4.4.1",
    "chai-as-promised": "8.0.2",
    "eslint": "9.39.1",
    "mocha": "11.7.5",
    "prettier": "3.7.4",
    "sinon": "21.0.0",
    "sinon-chai": "4.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier",
    "eslint": "eslint --cache ./",
    "eslint:fix": "eslint --cache --fix ./",
    "prettier": "prettier --write {lib,test,examples,mock}/{**/*,*}.js index.js",
    "test": "mocha --require ./test/support.js 'test/**/*.js'",
    "update-deps": "ncu -u && npm install --legacy-peer-deps" 
  },
  "engines": {
    "node": ">= 18"
  },
  "license": "MIT"
}
